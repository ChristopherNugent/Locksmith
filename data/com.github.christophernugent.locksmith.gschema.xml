<?xml version="1.0" encoding="UTF-8"?>
<schemalist>

    <schema id="com.github.christophernugent.locksmith" path="/com/github/christophernugent/locksmith/">
        <key name="window-x" type="i">
            <default>-1</default>
            <summary>Most recent x position of Locksmith</summary>
            <description>Most recent x position of Locksmith</description>
        </key>
    
        <key name="window-y" type="i">
            <default>-1</default>
            <summary>Most recent y position of Locksmith</summary>
            <description>Most recent y position of Locksmith</description>
        </key>
        
        <key name="open-tab" type="s">
            <default>"word"</default>
        </key>
        
        <key name="char-password" type="s">
            <default>""</default>
        </key>
        
        <key name="char-length" type="i">
            <default>128</default>
        </key>
        
        <key name="char-alpha" type="b">
            <default>true</default>
        </key>
        
        <key name="char-numeric" type="b">
            <default>true</default>
        </key>

        <key name="char-special" type="b">
            <default>true</default>
        </key>

        <key name="word-password" type="s">
            <default>""</default>
        </key>
        
        <key name="word-length" type="i">
            <default>4</default>
        </key>
        
        <key name="word-mode" type="i">
            <default>0</default>
        </key>
    </schema>

</schemalist>
